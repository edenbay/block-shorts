{
    "manifest_version": 3,
    "name": "Youtube Shorts blocker",
    "version": "1.0",
    "icons": {},
    "action": {},
    "permissions": ["scripting","activeTab"],
    "content_scripts": [
        {
        "js": ["scripts/content.js"],
        "run_at": "document_end",
        "matches": 
        [
            "https://www.youtube.com/*"
        ]
        }
    ]

  }